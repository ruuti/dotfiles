<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
"http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.ruuti.im-notify</string>
        <key>KeepAlive</key>
        <true/>
        <key>StandardErrorPath</key>
        <string>/tmp/.im-notify-stderr.log</string>
        <key>StandardOutPath</key>
        <string>/tmp/.im-notify-stdout.log</string>
        <key>ProgramArguments</key>
        <array>
            <string>/usr/local/bin/im-notify</string>
            <string>/bin/bash</string>
            <string>-c</string>
            <string>/usr/local/bin/im</string>
         </array>
         <key>RunAtLoad</key>
         <true/>
    </dict>
</plist>
